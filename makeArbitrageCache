# Limit how many items we generate
MAX=25

# Prefer newer entries (~50 lines per full scan)
LATEST=200

echo "local ItemIDs = {"
tail -${LATEST} ~/dev/wow/generated/arbitrage.log | sort | uniq -c | sort -nr | head -${MAX} | sed "s/^.*    /    /1"
echo "}"
echo
echo "ArbitrageCache = {"
echo "    ItemIDs = ItemIDs,"
echo "}"
